<%- include("partials/head") %>
<h1>All Registered</h1> 
<table border="1">
    <tr>
        <th>Username</th>
        <th>email</th>
        <th>gender</th>
        <th>roll</th>
        <th>option</th>
    </tr>
    <% customers = customers.map(el => { %> 

        <tr>
            <td><%= el.username %> </td>
            <td><%= el.email %> </td>
            <td><%= el.gender %> </td>
            <td><%= el.roll %> </td>
            <td>
                <a href="/register/data/edit/<%= el.id %> ">Edit</a> |
                <a href="/register/data/delete/<%= el.id %> ">Delete</a>
            </td>
        </tr>

   <% }) %> 
</table>
<%- include("partials/foot") %> 
